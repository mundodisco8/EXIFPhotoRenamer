[run]
; Only include files in the src subfolder in the test
include=src/**
; Alternatively, not include the venv files, which are in ~/.pyenv
omit=
    */.pyenv/**
    src/test/**
    # autogenerated Qt Code
    src/ui/**
; branch = True

[report]
# Skip empty __init__.py files
skip_empty = true

[xml]
output = coverageReport/coverage.xml